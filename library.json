{
    "name": "TRT-Astra",
    "version": "3.1.3",
    "description": "Terrapin Rocket Team's Multi-Mission Flight Software. Designed to be used with any FC and any sensor suite.",
    "keywords": "Astra, TRT, Terrapin Rocket Team, Flight Software",
    "repository": 
    {
        "type": "git",
        "url": "https://github.com/Terrapin-Rocket-Team/Multi-Mission-Flight-Software.git"
    },
    "authors": [
        {
            "name": "Drew Brandt",
            "url": "https://github.com/DrewBrandt"
        },
        {
            "name": "Michael Mallamaci",
            "url": "https://github.com/mallamacimj"
        },
        {
            "name": "Varun Unnithan",
            "url": "https://github.com/varun-un"
        },
        {
            "name": "Joseph Hauerstein",
            "url": "https://github.com/jhauerst"
        },
        {
            "name": "Abhi Senthilkumar",
            "url": "https://github.com/abhi-senthilkumar"
        },
        {
            "name": "Avi Komarlingam",
            "url": "https://github.com/avikomarlingam"
        },
        {
            "name": "Ramy Kaddouri",
            "url": "https://github.com/rk234"
        },
        {
            "name": "Krish Vyas",
            "url": "https://github.com/klzsh"
        },
        {
            "name": "Terrapin Rocket Team",
            "url": "https://github.com/Terrapin-Rocket-Team"
        }
    ],
    "license": "MIT",
    "homepage": "https://github.com/Terrapin-Rocket-Team/Multi-Mission-Flight-Software",
    "dependencies": [
        {
            "name": "Adafruit BMP280 Library",
            "owner": "adafruit"
        },
        {
            "name": "Adafruit BMP3XX Library",
            "owner": "adafruit"
        },
        {
            "name": "Adafruit BNO055",
            "owner": "adafruit"
        },
        {
            "name": "SparkFun u-blox GNSS v3",
            "owner": "sparkfun"
        },
        {
            "name": "RTClib",
            "owner": "adafruit"
        },
        {
            "name": "Adafruit DPS310",
            "owner": "adafruit"
        },
        {
            "name": "LIS3MDL",
            "owner": "pololu"
        },
        {
            "name": "Adafruit LIS331",
            "owner": "adafruit"
        },
        {
            "name": "Adafruit MMC56x3",
            "owner": "adafruit"
        },
        {
            "name": "MS5611",
            "version": "https://github.com/RobTillaart/MS5611.git#0.4.1"
        },
        {
            "name": "RadioMessage",
            "version": "https://github.com/erodarob/RadioMessage.git"
        },
        {
            "name": "BMI088",
            "version": "https://github.com/redstonee/bmi088-arduino-esp32.git"
        },
        {
            "name": "Adafruit ADXL375",
            "owner": "adafruit"
        },
        {
            "name": "SdFat",
            "version": "https://github.com/greiman/SdFat.git#2.3.1"
        }
    ],
    "frameworks": "arduino",
    "platforms": "*",
    "build": {
        "libLDFMode": "deep+",
        "libCompatMode": "strict",
        "srcFilter": [
            "+<*>"
        ],
        "flags": "-Wno-unknown-pragmas -std=gnu++17",
        "extraScript": "get_version.py",
        "unflags": "-std=gnu++11"
    },
    "export": {
        "exclude": [
            "examples",
            "test"
        ]
    },
    "platforms": {
        "ststm32": {
            "build": {
                "srcFilter": [
                    "+<*>",
                    "-<RecordData/Logging/SDCard/SDCardLog.cpp>"
                ]
            }
        },
        "espressif32": {
            "build": {
                "srcFilter": [
                    "+<*>"
                ]
            }
        }
    }
}